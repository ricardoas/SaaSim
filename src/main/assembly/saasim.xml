<assembly
	xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2 http://maven.apache.org/xsd/assembly-1.1.2.xsd">
	<id>saasim</id>
	<formats>
		<format>zip</format>
	</formats>
	<baseDirectory>saasim</baseDirectory>
	<fileSets>
		<fileSet>
			<outputDirectory></outputDirectory>
			<directory>scripts/bin/</directory>
			<includes>
				<include>*</include>
			</includes>
		</fileSet>
	</fileSets>
	<files>
		<file>
			<source>README</source>
		</file>
		<file>
			<source>config.properties</source>
		</file>
		<file>
			<source>iaas.plan</source>
		</file>
		<file>
			<source>iaas.providers</source>
		</file>
		<file>
			<source>saas.app</source>
		</file>
		<file>
			<source>saas.plans</source>
		</file>
		<file>
			<source>saas.users</source>
		</file>
	</files>
	<dependencySets>
		<dependencySet>
			<outputDirectory>/lib</outputDirectory>
			<scope>runtime</scope>
		</dependencySet>
	</dependencySets>
	<componentDescriptors />
</assembly>