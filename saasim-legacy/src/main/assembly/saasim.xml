<assembly
	xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2 http://maven.apache.org/xsd/assembly-1.1.2.xsd">
	<id>all</id>
	<formats>
		<format>zip</format>
	</formats>
	<baseDirectory>saasim</baseDirectory>
	<fileSets>
		<fileSet>
			<outputDirectory></outputDirectory>
			<directory>src/main/resources</directory>
			<includes>
				<include>examples/**</include>
				<include>planningtemplates/**</include>
				<include>log4j.properties</include>
			</includes>
			<excludes>
				<exclude>scripts</exclude>
			</excludes>
		</fileSet>
		<fileSet>
			<outputDirectory></outputDirectory>
			<directory>src/main/resources/scripts/bin</directory>
			<includes>
				<include>*</include>
			</includes>
		</fileSet>
		<fileSet>
			<outputDirectory></outputDirectory>
			<directory>src/main/resources/scripts/planning</directory>
			<includes>
				<include>*</include>
			</includes>
		</fileSet>
		<fileSet>
			<outputDirectory></outputDirectory>
			<directory>src/main/resources/scripts/provisioning</directory>
			<includes>
				<include>*</include>
			</includes>
		</fileSet>
		<fileSet>
			<outputDirectory></outputDirectory>
			<directory>src/main/java</directory>
			<includes>
				<include>log4j.properties</include>
			</includes>
		</fileSet>
	</fileSets>
	<files>
		<file>
			<source>README</source>
		</file>
	</files>
	<dependencySets>
		<dependencySet>
			<outputDirectory>/lib</outputDirectory>
			<scope>runtime</scope>
		</dependencySet>
	</dependencySets>
	<componentDescriptors />
</assembly>